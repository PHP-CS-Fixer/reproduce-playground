{
    "name": "keradus/php-cs-fixer-repro-cases",
    "require": {
        "php": "^8.1",
        "friendsofphp/php-cs-fixer": "3.13.1",
        "jubeki/laravel-code-style": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "keradus\\PhpCsFixerReproCases\\": "src/"
        }
    },
    "scripts": {
        "repro": "./vendor/bin/php-cs-fixer fix --using-cache=no --format=json --config=.php-cs-fixer.dist.php --path-mode=override .php-cs-fixer.dist.php"
    }
}
